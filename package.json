{
  "name": "vuetify-dashboard-boilerplate",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "build": "vue-cli-service build --modern",
    "build:dev": "vue-cli-service build --modern --mode development",
    "build:staging": "vue-cli-service build --modern --mode staging",
    "test:unit": "cross-env VUE_APP_TEST=unit vue-cli-service test:unit",
    "test:e2e": "cross-env VUE_APP_TEST=e2e vue-cli-service test:e2e --headless",
    "lint": "run-s lint:all:*",
    "build:ci": "npm run build --report",
    "dev": "vue-cli-service serve --open",
    "dev:e2e": "cross-env VUE_APP_TEST=e2e vue-cli-service test:e2e --mode=development",
    "stag": "vue-cli-service serve --open --mode staging",
    "stag:e2e": "cross-env VUE_APP_TEST=e2e vue-cli-service test:e2e --mode staging",
    "i18n:report": "vue-cli-service i18n:report --src './src/**/*.?(js|vue)' --locales './src/locales/**/*.json'",
    "lint:all:eslint": "eslint --fix --ext .js,.vue .",
    "lint:all:markdownlint": "npm run lint:markdownlint \"docs/*.md\" \"*.md\"",
    "lint:all:prettier": "npm run lint:prettier \"**/*.{js,json,css,scss,vue,html,md}\"",
    "lint:all:stylelint": "npm run lint:stylelint \"src/**/*.{vue,scss}\"",
    "lint:eslint": "eslint --fix",
    "lint:markdownlint": "markdownlint",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:stylelint": "stylelint --fix",
    "new": "cross-env HYGEN_TMPLS=generators hygen new",
    "test": "run-s test:unit test:e2e",
    "test:unit:ci": "npm run test:unit --coverage --ci",
    "test:unit:file": "npm run test:unit --bail --findRelatedTests",
    "test:unit:watch": "npm run test:unit --watch --notify --notifyMode change"
  },
  "dependencies": {
    "@casl/ability": "^4.1.6",
    "@casl/vue": "^1.1.1",
    "axios": "^0.20.0",
    "core-js": "3.6.5",
    "lodash": "^4.17.20",
    "lru-cache": "^6.0.0",
    "moment": "^2.29.1",
    "moment-duration-format": "^2.3.2",
    "nprogress": "0.2.0",
    "sortablejs": "^1.12.0",
    "vue": "2.6.12",
    "vue-clipboard2": "^0.3.1",
    "vue-ctk-date-time-picker": "^2.5.0",
    "vue-input-facade": "^1.3.2",
    "vue-loading-overlay": "^3.4.2",
    "vue-meta": "2.4.0",
    "vue-router": "3.4.3",
    "vue-upload-component": "^2.8.20",
    "vuetify": "^2.3.13",
    "vuetify-dialog": "^2.0.12",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@intlify/vue-i18n-loader": "^1.0.0",
    "@vue/cli-plugin-babel": "^4.5.7",
    "@vue/cli-plugin-e2e-cypress": "^4.5.7",
    "@vue/cli-plugin-eslint": "^4.5.7",
    "@vue/cli-plugin-unit-jest": "^4.5.7",
    "@vue/cli-service": "^4.5.7",
    "@vue/eslint-config-prettier": "6.0.x",
    "@vue/eslint-config-standard": "5.1.x",
    "@vue/test-utils": "1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.x",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-require-context-hook": "^1.0.0",
    "cross-env": "7.0.x",
    "eslint": "7.9.x",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "11.1.x",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "~4.2.1",
    "eslint-plugin-standard": "4.0.x",
    "eslint-plugin-vue": "6.2.x",
    "eslint-plugin-vuetify": "^1.0.0-beta.7",
    "express": "4.17.x",
    "hygen": "5.0.3",
    "imagemin-lint-staged": "0.4.x",
    "lint-staged": "10.3.x",
    "markdownlint-cli": "0.23.x",
    "moment-locales-webpack-plugin": "^1.2.0",
    "npm-run-all": "4.1.x",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.3",
    "stylelint": "^13.7.2",
    "stylelint-config-css-modules": "2.2.x",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "2.1.x",
    "stylelint-config-standard": "20.0.x",
    "stylelint-scss": "3.18.x",
    "vue-cli-plugin-i18n": "~1.0.1",
    "vue-cli-plugin-vuetify": "~2.0.7",
    "vue-template-compiler": "2.6.12",
    "vuetify-loader": "^1.6.0",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12"
  },
  "gitHooks": {
    "pre-commit": "cross-env PRE_COMMIT=true lint-staged"
  },
  "resolutions": {
    "@vue/cli-plugin-unit-jest/jest": "25.1.x",
    "@vue/cli-plugin-unit-jest/babel-jest": "25.1.x"
  }
}
